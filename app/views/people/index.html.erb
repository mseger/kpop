<!-- Roommates page -->
<div class="roommates-page">
	<%= render partial: 'users/edit' %>

	<div class="matches-container">
		<div class="well">
			<h2>Your Matches</h2>
			<div class="matches-carousel-container">
		  	<%= render partial: 'matches_carousel', locals: {matches: @matches, matchees: @matchees} %>
		  </div>
	  </div>
	  <div class="well">
	  	<h2>Your Friends in the Area</h2>
	  	<div class="fb-matches-carousel-container">
		  	<%= render partial: 'matches_carousel', locals: {matches: @fb_matches, matchees: @fb_matchees} %>
		  </div>
	  </div>
	</div>