<div class="matches-carousel">
  <div class="row-fluid">
    <ul class="pager">
      <li class="span2">
        <%= link_to_previous_page matches, "Prev", class: "pagination-prev", params: {carousel_name: carousel_name}, remote: true %>
      </li>
    <% matchees.zip(matches).each do |matchee, match| %>
      <li class="span2 match-card-container" data-matchee-id="<%= matchee.id %>">
        <%= render partial: "people/match_card", locals: {matchee: matchee, match: match} %>
      </li>
    <% end %>

      <li class="span2">
        <%= link_to_next_page matches, "Next", class: "pagination-next", params: {carousel_name: carousel_name}, remote: true %>
      </li>
    </ul>
  </div>
</div>